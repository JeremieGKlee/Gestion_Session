{{ form_start(form) }}
        <div class="row">
            <div class="col-md-4">{{ form_row(form.module) }}</div>
            <div class="col-md-4">{{ form_row(form.duration) }}</div>
            <div class="col-md-4">{{ form_row(form.session) }}</div>
        </div>
        {{ form_rest(form) }}
        <button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
        {{ form_end(form) }}