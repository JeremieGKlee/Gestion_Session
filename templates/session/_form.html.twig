{{ form_start(form) }}
    <div class="row">
        <div class="col-md-4">{{ form_row(session.title) }}</div>
    </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(session.datestart) }}</div>
    </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(session.dateend) }}</div>
    </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(session.spaceavailable) }}</div>
    </div>
    {{ form_rest(form) }}
    <button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
{{ form_end(form) }}